+          beqz(ih, FW_Y0_NO_NEGATION);
+          fneg_d(v4, v4);
+        bind(FW_Y0_NO_NEGATION);
+
+        block_comment("for (i=1;i<=jz;i++) fw += fq[i];"); {
+            li(i, 1);
+            blt(jz, i, FW_FOR2_DONE);
+          bind(FW_FOR2);
+            alsl_d(tmp5, i, SCR2, 3 - 1);
+            fld_d(v1, tmp5, 0);
+            addi_w(i, i, 1);
+            fadd_d(v5, v5, v1);
+            bge(jz, i, FW_FOR2);
+        }
+        bind(FW_FOR2_DONE);
+          beqz(ih, FW_Y1_NO_NEGATION);
+          fneg_d(v5, v5);
+        bind(FW_Y1_NO_NEGATION);
+          addi_d(SP, SP, 560);
+      }
+}
