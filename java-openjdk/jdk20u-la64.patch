+  InternalAddress safepoint_pc(ce->masm()->pc() - ce->masm()->offset() + safepoint_offset());
+  __ lea(SCR2, safepoint_pc);
+  __ st_d(SCR2, Address(TREG, JavaThread::saved_exception_pc_offset()));
