+        masknez(AT, src1, AT);
+      } else {
+        masknez(dst, src1, AT);
+        maskeqz(AT, src2, AT);
+      }
+      break;
+
+    case GT:
+      if (is_signed) {
+        slt(AT, op2, op1);
+      } else {
+        sltu(AT, op2, op1);
+      }
+      if(dst == src2) {
+        maskeqz(dst, src2, AT);
+        masknez(AT, src1, AT);
+      } else {
