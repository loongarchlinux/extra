+void C2_MacroAssembler::cmp_branchEqNe_off21(int flag, Register op1, Label& L) {
+    switch(flag) {
+      case 0x01: //equal
+        beqz(op1, L);
+        break;
+      case 0x02: //not_equal
+        bnez(op1, L);
+        break;
+      default:
+        Unimplemented();
+    }
