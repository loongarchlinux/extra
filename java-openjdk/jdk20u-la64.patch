+      } else {
+        sltu(AT, op1, op2);
+      }
+      if(dst == src2) {
+        masknez(dst, src2, AT);
+        maskeqz(AT, src1, AT);
+      } else {
+        maskeqz(dst, src1, AT);
+        masknez(AT, src2, AT);
+      }
+      break;
+
+    case LT:
+      if (is_signed) {
+        slt(AT, op1, op2);
+      } else {
+        sltu(AT, op1, op2);
+      }
+      if(dst == src2) {
+        maskeqz(dst, src2, AT);
+        masknez(AT, src1, AT);
+      } else {
+        masknez(dst, src1, AT);
+        maskeqz(AT, src2, AT);
+      }
+      break;
+    case LE:
+      if (is_signed) {
+        slt(AT, op2, op1);
+      } else {
+        sltu(AT, op2, op1);
+      }
+      if(dst == src2) {
+        masknez(dst, src2, AT);
