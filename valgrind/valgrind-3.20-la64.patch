+/*---------------------------------------------------------------*/
+/*--- Vex's representation of the LOONGARCH64 CPU state.      ---*/
+/*---------------------------------------------------------------*/
+
+typedef
+   struct {
+      /* Event check fail addr and counter. */
+      /*    0 */ ULong host_EvC_FAILADDR;
+      /*    8 */ UInt  host_EvC_COUNTER;
+      /*   12 */ UInt  _padding0;
+
+      /* CPU Registers */
+      /*   16 */ ULong guest_R0;   /* Constant zero */
