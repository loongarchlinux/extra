+/*------------------------------------------------------------*/
+/*--- Signal frame layouts                                 ---*/
+/*------------------------------------------------------------*/
+
+struct vg_sig_private {
+   UInt magicPI;
+   UInt sigNo_private;
+   VexGuestLOONGARCH64State vex_shadow1;
+   VexGuestLOONGARCH64State vex_shadow2;
+};
+
+struct rt_sigframe {
+   struct vki_siginfo rs_info;
