+static struct valgrind_target_ops low_target = {
+   num_regs,
+   regs,
+   3, // SP
+   transfer_register,
+   get_pc,
+   set_pc,
+   "loongarch64",
+   target_xml,
+   target_get_dtv
+};
+
+void loongarch64_init_architecture (struct valgrind_target_ops* target)
+{
+   *target = low_target;
+   set_register_cache (regs, num_regs);
+   gdbserver_expedite_regs = expedite_regs;
+}
