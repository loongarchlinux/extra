+   // __NR_times 43
+   GO(__NR_times, "1s 1m");
+   SY(__NR_times, x0+1); FAIL;
+
+   // __NR_prof 44
+   //GO(__NR_prof, "ni");
+   //SY(__NR_prof); FAIL;
